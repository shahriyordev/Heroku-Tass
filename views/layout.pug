  body.fixed-sn.white-skin

    header
      // Sidebar navigation
      #slide-out.side-nav.sn-bg-4.fixed
        ul.custom-scrollbar
          li.logo-sn.waves-effect
            .text-center
              a.pl-0(href='#')
                img(src='/splash_sh.png').rasm
          li
            form.search-form(role='search')
              .form-group.md-form.mt-0.pt-1.waves-light
                input.form-control(type='text' placeholder='Search')
          // /.Search Form
          // Side navigation links
          if user 
            li
              ul.collapsible.collapsible-accordion
                li
                  a.collapsible-header.waves-effect.arrow-r(href='/dashboard')
                    i.fas.fa-tachometer-alt
                    |                                 Dashboards
                    i.fas.fa-angle-down.rotate-icon
                li
                  a.collapsible-header.waves-effect.arrow-r(href='/organization')
                    i.fas.fa-image
                    |  Organization
                    i.fas.fa-angle-down.rotate-icon
                li
                  a.collapsible-header.waves-effect.arrow-r(href='/music/add')
                    i.fas.fa-user
                    |                                 Users
                    i.fas.fa-angle-down.rotate-icon
                li
                  a.collapsible-header.waves-effect.arrow-r(href='/debug')
                    i.fa.fa-exclamation-circle.mr-2(aria-hidden='true')
                    |                                 Debug
                    i.fas.fa-angle-down.rotate-icon
                li
                  a.collapsible-header.waves-effect.arrow-r(href='/devices')
                    i.fas.fa-th
                    |                                 Devices
                    i.fas.fa-angle-down.rotate-icon
          .sidenav-bg.mask-strong
      // /. Sidebar navigation
      // Navbar
      nav.navbar.fixed-top.navbar-expand-lg.scrolling-navbar.double-nav
        // Navbar links
        ul.nav.navbar-nav.nav-flex-icons.ml-auto
          li.nav-item.dropdown
            a#userDropdown.nav-link.dropdown-toggle.waves-effect(href='#' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
              i.fas.fa-user
              span.clearfix.d-none.d-sm-inline-block Profile
            .dropdown-menu.dropdown-menu-right(aria-labelledby='userDropdown')
              if user
                a.dropdown-item(href='/logout') Log Out
              else
                a.dropdown-item(href='/login') Sign in
                a.dropdown-item(href='/register') Resgiter
      .container 
        .row 
          .col-lg-6
            != messages('message', locals)
            if errors 
              each error, i in errors 
                div(class='alert alert-danger') #{error.msg}

